{
  "name": "dnhyxc-ui",
  "version": "0.0.1",
  "scripts": {
    "build": "gulp -f packages/components/scripts/build/index.ts",
    "dev:docs": "pnpm -C docs dev",
    "build:docs": "pnpm -C docs build",
    "preview:docs": "pnpm -C docs preview",
    "dev:play": "pnpm -C play dev",
    "vitest": "pnpm -C packages/components test",
    "vitest:coverage": "pnpm -C packages/components coverage",
    "vitest:component": "pnpm -C packages/components run test:component",
    "publish": "pnpm run build && pnpm changeset && pnpm changeset version && pnpm changeset publish && tsx packages/components/scripts/version/index.ts",
    "prepare": "husky install",
    "create": "NODE_NO_WARNINGS=1 tsx packages/components/scripts/create/index.ts",
    "commit": "git-cz",
    "test": "eslint ./packages --ext ts,vue,js --fix"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "keywords": [
    "library",
    "vue3",
    "ui",
    "dnhyxc-ui-plus"
  ],
  "author": "@dnhyxc",
  "license": "ISC",
  "description": "a ui library for vue3",
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.5",
    "@changesets/cli": "^2.29.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@dnhyxc/eslint": "^0.0.9",
    "@eslint/js": "^9.30.1",
    "@types/gulp": "^4.0.17",
    "@types/gulp-autoprefixer": "^0.0.37",
    "@types/gulp-clean-css": "^4.3.4",
    "@types/gulp-sass": "^5.0.4",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/test-utils": "^2.4.6",
    "chalk": "^5.6.2",
    "commitizen": "^4.3.1",
    "cz-customizable": "^7.4.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "gulp": "^5.0.1",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^6.0.1",
    "happy-dom": "^18.0.1",
    "husky": "^8.0.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2",
    "sucrase": "^3.35.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.35.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-element-plus": "^0.10.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0"
  },
  "dependencies": {
    "element-plus": "^2.10.4",
    "vue": "^3.5.17"
  }
}